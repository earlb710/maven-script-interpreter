// Test script to reproduce the qualified name issue in call statements

// Create a simple namespace structure using variable sets
var VisibleSet {
    var string testVar1 = "visible1";
}

var scopeVisibleTest {
    var VisibleSet;
}

// Define debug.assert function
block #debug {
    block #assert(bool condition, string message) {
        if (condition) then {
            print "✓ " + message;
        } else {
            print "✗ " + message;
        }
    }
}

// Test 1: Simple qualified name call
call #debug.assert(true, "TEST 0: Simple call works");

// Test 2: Call with nested qualified variable reference (the problem case)
call #debug.assert(scopeVisibleTest.VisibleSet.testVar1 == "visible1", "TEST 1: Value should be 'visible1'");

print "Test completed";
