    // White back row (row 7)
    pieces[24] = ChessPiece { piece: "R", color: "W", pos: posType { x: 0, y: 7 } };
    pieces[25] = ChessPiece { piece: "N", color: "W", pos: posType { x: 1, y: 7 } };
