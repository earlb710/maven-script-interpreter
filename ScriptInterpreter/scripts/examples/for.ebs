// EBS Language Reference v1.4.0 - See EBS_LANGUAGE_REFERENCE.md
// Example: for loop - C-style iteration
// Syntax: for (init; condition; increment) { }

// Count from 1 to 5
for (var i: int = 1; i <= 5; i++) {
    print "Number: " + i;
}

// Step by 2
for (var j: int = 0; j <= 10; j += 2) {
    print "Even: " + j;
}
