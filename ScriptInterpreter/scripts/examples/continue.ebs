// EBS Language Reference v1.2.0 - See EBS_LANGUAGE_REFERENCE.md
// Example: continue keyword - Skip to next iteration
// Syntax: continue;

// Skip even numbers
for (var i: int = 1; i <= 5; i++) {
    if i == 3 then {
        continue;
    }
    print "Number: " + i;
}

// Output: 1, 2, 4, 5 (skips 3)
