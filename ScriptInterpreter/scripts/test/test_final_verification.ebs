// Final verification of all modulo features
print("Final Modulo Feature Verification");
print("==================================");

// Feature 1: % operator
var r1: int = 10 % 3;
print("10 % 3 = " + r1);

// Feature 2: mod keyword
var r2: int = 10 mod 3;
print("10 mod 3 = " + r2);

// Feature 3: %= compound assignment
var x: int = 17;
x %= 5;
print("17 %= 5: x = " + x);

// Feature 4: Mixed usage
var count: int = 25;
if count mod 2 == 1 then {
    print(count + " is odd");
    count %= 10;
    print("After %= 10: count = " + count);
}

print("");
print("âœ“ All modulo features verified successfully!");
